<?php if (is_array($this->data)):?><?php if (strlen($this->title)):?><h3 class="report-chart-title"><?php echo _($this->title) ?></h3><?php endif;?><table><?php $i = 0;?><?php foreach ($this->data as $num => $row): ?><tr><?php foreach ($row as $cell): ?><?php if (!$i): ?><th><?php echo $cell;?></th><?php else: ?><td><?php echo ($cell === false) ? '-' : $cell;?></td><?php endif;?><?php endforeach;?></tr><?php $i++; ?><?php endforeach;?></table><?php endif;?>